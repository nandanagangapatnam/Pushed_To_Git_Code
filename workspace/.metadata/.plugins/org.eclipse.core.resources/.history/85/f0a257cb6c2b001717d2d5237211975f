	// Run Reports  - working fine
	@Test (priority = 5)
	public void _5_Reportsgeneration() throws Exception {		

		Thread.sleep(1000);
		xpath =".//*[@id='Left']/a[8]";  // Reports 
		new WebDriverWait(driver, 10).until(ExpectedConditions.visibilityOfElementLocated(By.xpath(xpath))).click();
		
		xpath =".//*[@id='ctl00_MainContent_rptMenu_ctl06_lnkMenu']";  // Reports 
		new WebDriverWait(driver, 10).until(ExpectedConditions.visibilityOfElementLocated(By.xpath(xpath))).click();
	
		xpath =".//*[@id='ctl00_MainContent_rptReport_ctl02_lnkReport']";  // AML Log Report V2 
		new WebDriverWait(driver, 10).until(ExpectedConditions.visibilityOfElementLocated(By.xpath(xpath))).click();
		
		xpath =".//*[@id='ctl00_MainContent_ctl00_dbDefDateBegin_txtDate']";  // Begin Xaction Date
		new WebDriverWait(driver, 10).until(ExpectedConditions.visibilityOfElementLocated(By.xpath(xpath))).clear();
		new WebDriverWait(driver, 10).until(ExpectedConditions.visibilityOfElementLocated(By.xpath(xpath))).sendKeys("5/28/2008");
		
		xpath =".//*[@id='ctl00_MainContent_ctl00_dbDefDateEnd_txtDate']";  // End Xaction Date
		new WebDriverWait(driver, 10).until(ExpectedConditions.visibilityOfElementLocated(By.xpath(xpath))).clear();
		new WebDriverWait(driver, 10).until(ExpectedConditions.visibilityOfElementLocated(By.xpath(xpath))).sendKeys("5/28/2008");

		xpath =".//*[@id='ctl00_MainContent_ctl00_txtAmountBegin']";  // Begin Threshold Amount
		new WebDriverWait(driver, 10).until(ExpectedConditions.visibilityOfElementLocated(By.xpath(xpath))).clear();
		new WebDriverWait(driver, 10).until(ExpectedConditions.visibilityOfElementLocated(By.xpath(xpath))).sendKeys("3000");
		
		xpath =".//*[@id='ctl00_MainContent_ctl00_txtAmountEnd']";  // End Threshold Amount
		new WebDriverWait(driver, 10).until(ExpectedConditions.visibilityOfElementLocated(By.xpath(xpath))).clear();
		new WebDriverWait(driver, 10).until(ExpectedConditions.visibilityOfElementLocated(By.xpath(xpath))).sendKeys("1500000");
		
		xpath =".//*[@id='ctl00_MainContent_ctl00_btnSubmit']";  // Run Report 
		new WebDriverWait(driver, 10).until(ExpectedConditions.visibilityOfElementLocated(By.xpath(xpath))).click();
		
        Set<String> windowId = driver.getWindowHandles();    // get  window id of current window
        Iterator<String> itererator = windowId.iterator();   

        String mainWinID = itererator.next();
        String  newWinID = itererator.next();

        driver.switchTo().window(newWinID);
        System.out.println(driver.getTitle());
        Thread.sleep(2000);
        driver.close();

        driver.switchTo().window(mainWinID);
        System.out.println(driver.getTitle());
 /*     Thread.sleep(2000);
        driver.close(); */
			
	}
